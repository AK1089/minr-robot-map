# FUNCTION | checkSolution(Player player, Entity entity, Int index) -> Boolean

@if index == 0
    @return util::executeAndQueryResult("/execute if data entity @e[tag=claude_level_0_check,limit=1] Items[0]") > 0
@elseif index == 1
    @return util::executeAndQueryResult("/execute if data entity @e[tag=claude_level_1_check,limit=1] Items[0]") == 12
@fi

